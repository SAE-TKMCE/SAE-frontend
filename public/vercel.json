{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'none';"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; media-src 'self' https://sae.tkmce.ac.in https://your-storage-domain.com;"
        }
      ]
    }
  ]
}
